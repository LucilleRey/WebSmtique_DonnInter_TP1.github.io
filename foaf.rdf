<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="foaf-to-xhtml.xsl"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:foaf="http://xmlns.com/foaf/0.1/">

  <!-- Déclaration d'une personne -->
  <foaf:Person rdf:about="https://lucillerey.github.io/foaf.rdf">
    
    <!-- Nom complet -->
    <foaf:name>Jean Dupont</foaf:name>
    <!-- Titre -->
    <foaf:title>Mr.</foaf:title>
    <!-- Nom donné -->
    <foaf:givenName>Jimmy</foaf:givenName>
    <!-- Nom de famille -->
    <foaf:familyName>Wales</foaf:familyName>
    <!-- Adresse e-mail -->
    <foaf:mbox rdf:resource="mailto:jWales@example.com"/>
    
    <!-- Page d'accueil (homepage) -->
    <foaf:homepage rdf:resource="https://lucillerey.github.io"/>
    
    <!-- Nom d'utilisateur -->
    <foaf:nick>Jimbo</foaf:nick>
    
    <!-- Image du profil -->
    <foaf:img rdf:resource="https://lucillerey.github.io/images/photo.jpg"/>

    <!-- Une brève description -->
    <foaf:title>Personnage fictif crée pour un tp à partir des données de wikipédia</foaf:title>

  </foaf:Person>

</rdf:RDF>